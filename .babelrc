{
    "presets": [
        ["@babel/preset-env", {
            "useBuiltIns": "usage",
            "corejs": 3,
            "forceAllTransforms": true,
            "targets": {
                "browsers": ["> 0.25%", "not dead"]
            }
        }]
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {"loose": false}],
        "@babel/plugin-transform-flow-strip-types"
    ]
}